//To Print All Even Numbers Till N
#include<stdio.h>
main()
{
	int x,n;
	printf("Enter The Number To Find Even Numbers Upto N\n");
	scanf("%d",&n);
	for(x=1;x<=n;x++)
	if(x%2==0)
	{
		printf("%d\n",x);
	}
	return 0;
}
//To Print All Odd Numbers Till N
#include<stdio.h>
main()
{
	int x,n;
	printf("Enter The Number To Find Even Numbers Upto N\n");
	scanf("%d",&n);
	for(x=1;x<=n;x++)
	if(x%2==!0)
	{
		printf("%d\n",x);
	}
	return 0;
}
